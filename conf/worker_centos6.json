{
    "Hostname": "centos6-worker",
    "Domainname": "to.infn.it",
    "User": "",
    "AttachStdin": false,
    "AttachStdout": false,
    "AttachStderr": false,
    "Tty": false,
    "OpenStdin": false,
    "StdinOnce": false,
    "Env": null,
    "Cmd": [
      "/bin/sleep", 
      "200"
      ],
    "Image": "mconcas/centos6-autobuild-container:latest",
    "WorkingDir": "/",
    "NetworkDisabled": false,
    "ExposedPorts": {},
    "HostConfig": {
      "Memory": 0,
      "MemorySwap": 0,
      "CpuShares": 512,
      "CpuPeriod": 100000,
      "CpusetCpus": "0,1",
      "BlkioWeight": 300,
      "OomKillDisable": false,
      "PublishAllPorts": false,
      "Privileged": false,
      "ReadonlyRootfs": false,
      "ExtraHosts": null,
      "CapAdd": ["NET_ADMIN"],
      "CapDrop": ["MKNOD"],
      "RestartPolicy": { "Name": "", "MaximumRetryCount": 0 },
      "NetworkMode": "bridge",
      "Devices": [],
      "LogConfig": { "Type": "json-file", "Config": {} }
   }
}
